<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.localdealguys" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Ticket Fetch</name>
    <description>Discount Show tickets direct to your phone!</description>
    <author email="support@ticketfetch.com" href="http://www.ticketfetch.com">TicketFetch Team</author>
    <content src="index.html" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <splash src="splash.png" />
    <preference name="SplashScreen" value="screen" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="SplashScreenDelay" value="5000" />
</widget>
